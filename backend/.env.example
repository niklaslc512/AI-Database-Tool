# 日志系统配置
# 日志级别: error, warn, info, debug, trace
LOG_LEVEL=info

# 运行环境配置
NODE_ENV=development

# 日志文件配置
LOG_MAX_SIZE=20m
LOG_MAX_FILES=15d
LOG_ENABLE_COMPRESSION=true

# 日志目录配置
LOG_DIR=./logs

# 日志审计配置
LOG_AUDIT_ENABLED=true

# 开发环境特定配置
# 在开发环境下启用调试日志和控制台输出
DEV_LOG_CONSOLE=true
DEV_LOG_DEBUG=true

# 生产环境特定配置（当NODE_ENV=production时生效）
# PROD_LOG_LEVEL=warn
# PROD_LOG_MAX_SIZE=50m
# PROD_LOG_MAX_FILES=30d

# ai llm 配置
AI_LLM_MODEL=
AI_LLM_BASE_URL=
AI_LLM_API_KEY=
